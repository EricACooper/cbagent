div.charts_panel
    div.cpanel_left
        div.btn-group(data-toggle="buttons-radio")
            button#view1.btn.btn-mini.vradio(type="button") 1 view
            button#view2.btn.btn-mini.vradio(type="button") 2 views
            button#view4.btn.btn-mini.vradio(type="button") 4 views
    div.cpanel_middle
        button#clear_views.btn.btn-mini Clear All
    div.cpanel_right
        div.btn-group(data-toggle="buttons-radio")
            button#minute.btn.btn-mini(type="button") minute
            button#hour.btn.btn-mini(type="button") hour
            button#day.btn.btn-mini(type="button") day
            button#week.btn.btn-mini(type="button") week
            button#month.btn.btn-mini(type="button") month
            button#all.btn.btn-mini(type="button") all
